@model IEnumerable<SalaryCapGame.Views.WebViewModels.FranchiseIndexListingModel>


<table id="franchiseTable" align="left" class="table row-border table-sm table-striped table-bordered" style="font-size:small; padding:0;" width:100%>
    <thead>
        <tr class="no-gutters">
            <th class="text-left">Franchise</th>
            <th class="text-right">Points</th>
            <th class="text-right">Value</th>
            <th class="text-right">Trades</th>
        </tr>
    </thead>
    <tbody>
        @foreach ( var item in Model )
        {
            <tr class="no-gutters">
                <td class="text-left">@Html.DisplayFor( i => item.Nickname )</td>
                <td class="text-right">@Html.DisplayFor( i => item.Points)</td>
                <td class="text-right">@Html.DisplayFor( i => item.Value )</td>

                <td class="text-right">@Html.DisplayFor( i => item.NumberOfTrades )</td>
               
            </tr>
        }
    </tbody>

</table>

