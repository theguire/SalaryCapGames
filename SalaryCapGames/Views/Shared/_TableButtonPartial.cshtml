<td style="width:150px;">
	<div class="btn-group" role="group">
		<a href="@Url.Action("Edit/"+Model)" type="button" class="btn btn-primary">
			<i class="fas fa-edit"></i>
		</a>
		<a href="@Url.Action("Details/"+Model)" type="button" class="btn btn-success">
			<i class="far fa-list-alt"></i>
		</a>
		<a href="@Url.Action("Delete/"+Model)" type="button" class="btn btn-danger">
			<i class="fas fa-trash-alt"></i>
		</a>
	</div>
</td>