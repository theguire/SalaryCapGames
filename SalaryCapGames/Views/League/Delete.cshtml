
@model SalaryCapData.Data.Models.League
@{
    ViewData[ "Title" ] = "Delete";
}

<br />
<br />
<h2>League Detail</h2>
<hr />

<form method="post">
    <div class="form-border">
        <input type="hidden" asp-for="Id"/>
        <fieldset disabled>
            <div class="form-group row">
                <div class="col-2">
                    <label class="custom-label" asp-for="Name"></label>
                </div>
                <div class="col-4">
                    <input asp-for="Name" class="form-control" />
                </div>
            </div>
            <div class="form-group row">
                <div class="col-2">
                    <label class="custom-label" asp-for="Commissioner"></label>
                </div>
                <div class="col-4">
                    <input asp-for="Commissioner" class="form-control" />
                </div>
            </div>
            <div class="form-group row">
                <div class="col-2">
                    <label class="custom-label" asp-for="MaxNumberFranchises"></label>
                </div>
                <div class="col-4">
                    <input asp-for="MaxNumberFranchises" class="form-control" />
                </div>
            </div>
            <div class="form-group row">
                <div class="col-2">
                    <label class="custom-label" asp-for="IsPrivate"></label>
                </div>
                <div class="col-4">
                    <input asp-for="IsPrivate" class="form-control" />
                </div>
            </div>
            <div class="form-group row">
                <div class="col-2">
                    <label class="custom-label" asp-for="Points"></label>
                </div>
                <div class="col-4">
                    <input asp-for="Points" class="form-control" />
                </div>
            </div>

            <div class="form-group row">
                <div class="col-2">
                    <label class="custom-label" asp-for="Value"></label>
                </div>
                <div class="col-4">
                    <input asp-for="Value" class="form-control" />
                </div>
            </div>
            <div class="form-group row">
                <div class="col-2">
                    <label class="custom-label" asp-for="DateCreated"></label>
                </div>
                <div class="col-4">
                    <input asp-for="DateCreated" class="form-control" />
                </div>
            </div>
            <div class="form-group row">
                <div class="col-2">
                    <label class="custom-label" asp-for="DateModified"></label>
                </div>
                <div class="col-4">
                    <input asp-for="DateModified" class="form-control" />
                </div>
            </div>
        </fieldset>


        <div class="form-group">
            <input type="submit" value="Delete" class="btn btn-danger"/>
            <a asp-action="Index" class="btn btn-success">Back to List</a>
        </div>
    </div>

</form>